{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Detect Image",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "detect",
        "./templates/heads_02.jpg",
        "--heads",
        "./templates/heads_03.jpg",
        "--tails",
        "./templates/tails_02.jpg"
      ]
    },
    {
      "name": "Debug: Camera",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "camera",
        "--heads",
        "./templates/heads_02.jpg",
        "--tails",
        "./templates/tails_01.jpg"
      ]
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
